@model Escuela

@{
    ViewData["Title"] = "Información Escuela";
}

<h1>@ViewData["Title"]</h1>
<h2>Nombre: @Model.Nombre </h2>
<h3>Tipo de Escuel: @Model.TipoEscuela</h3>
<address>
    <p>Dirección: @Model.Direccion</p>
    <p>País: @Model.Pais</p>
    <p>Ciudad: @Model.Ciudad</p>
</address>
<p><strong>Año fundación: @Model.AñoDeCreacion</strong></p>

<p><i>@ViewBag.CosaDinamica</i></p>